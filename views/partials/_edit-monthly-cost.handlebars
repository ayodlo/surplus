<div class="table-container">
    <div class="back-container costs-menu-button-left">
        <i class="fa fa-chevron-left"></i>
        <span class="back-text">BACK</span>
    </div>
    <table>
        <tr>
            <th>Cost</th>
            <th>Bought</th>
            <th>Date</th>
            <th></th>
            <th></th>
        </tr>
        {{#each monthsCosts}}
        <tr class="monthly-cost-list">
            <td>${{this}}</td>
            <td>{{@key}}</td>
            <td>7/8/19</td>
            <td></td>
            <td class="delete" onclick="deleteMonthlyCost('{{@key}}')">Delete</td>
        </tr>
        {{/each}}
    </table>
</div>
<form method="post" action="/costsMonthly/delete" class="theme-input-container" id="editMonthlyCostsForm">
    <input type="text" id="editMonthlyCostsInput" name="costsMonthlyIndex" value="">
    <input type="submit" value="Submit">
</form>